@model IEnumerable<WebsiteBH.Models.ApplicationUser>
@{
    ViewBag.Title = "Trang Chu Admin";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";


}
<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                @if (!Request.IsAuthenticated)
                {


                    <h1>Xin Chào </h1>
                }
                else
                {
                    <h1>Xin Chào: @User.Identity.Name</h1>
                }

            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->
